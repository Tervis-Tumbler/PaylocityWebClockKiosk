{
  "manifest_version": 2,
  "name": "Paylocity WebClock Kiosk",
  "minimum_chrome_version": "24.0.1307.0",
  "version": "0.5",
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "permissions": [
    "webview",
    "power"
  ],
  "kiosk_enabled":true
}
